<div class="fib-image">    <picture class="fib-pic">        <source srcset="{{ fib-imgs.large.high }}" media="(min-width: 1000px)">        <source srcset="{{ fib-imgs.large.high }}" media="(min-width: 800px)">        <source srcset="{{ fib-imgs.large.low }}">        <img srcset="{{ fib-imgs.large.low }}" alt="{{ fib-imgs.large.alt }}">    </picture>    <div class="fib-box">        <picture class="visible-smup medium-pic">            <source srcset="{{ fib-imgs.medium.high }}" media="(min-width: 1000px)">            <source srcset="{{ fib-imgs.medium.high }}" media="(min-width: 800px)">            <source srcset="{{ fib-imgs.medium.low }}">            <img srcset="{{ fib-imgs.medium.low }}" alt="{{ fib-imgs.medium.alt }}">        </picture>        <picture class="visible-xsup small-pic">            <source srcset="{{ fib-imgs.small.high }}" media="(min-width: 1000px)">            <source srcset="{{ fib-imgs.small.high }}" media="(min-width: 800px)">            <source srcset="{{ fib-imgs.small.low }}">            <img srcset="{{ fib-imgs.small.low }}" alt="{{ fib-imgs.small.alt }}">        </picture>        <picture class="visible-xsup small-pic">            <source srcset="{{ fib-imgs.small.high-dark }}" media="(min-width: 1000px)">            <source srcset="{{ fib-imgs.small.high-dark }}" media="(min-width: 800px)">            <source srcset="{{ fib-imgs.small.low-dark }}">            <img srcset="{{ fib-imgs.small.low-dark }}" alt="{{ fib-imgs.small.alt }}">        </picture>    </div></div>